<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="650.0" prefWidth="850.0" style="-fx-background-color: #171717;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Classes.HomeController">
   <children>
      <AnchorPane prefHeight="650.0" prefWidth="250.0" style="-fx-background-color: #ffc401;">
         <children>
            <ImageView fitHeight="250.0" fitWidth="250.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Imagens/Horus-Logo.png" />
               </image>
            </ImageView>
            <Button fx:id="homeButton" alignment="CENTER" layoutX="78.0" layoutY="273.0" mnemonicParsing="false" onAction="#homeStage" style="-fx-background-color: #171717;" text="Home" textFill="#ffc401">
               <font>
                  <Font name="Candara" size="24.0" />
               </font>
            </Button>
            <Button fx:id="buildingButton" alignment="CENTER" layoutX="68.0" layoutY="338.0" mnemonicParsing="false" style="-fx-background-color: #171717;" text="Building" textFill="#ffc401">
               <font>
                  <Font name="Candara" size="24.0" />
               </font>
            </Button>
            <Button fx:id="statsButton" alignment="CENTER" layoutX="52.0" layoutY="403.0" mnemonicParsing="false" style="-fx-background-color: #171717;" text="Estatisticas" textFill="#ffc401">
               <font>
                  <Font name="Candara" size="24.0" />
               </font>
            </Button>
            <Button fx:id="configsButton" alignment="CENTER" layoutX="36.0" layoutY="467.0" mnemonicParsing="false" style="-fx-background-color: #171717;" text="Configurações" textFill="#ffc401">
               <font>
                  <Font name="Candara" size="24.0" />
               </font>
            </Button>
            <Button fx:id="logoutButton" alignment="CENTER" layoutX="73.0" layoutY="596.0" mnemonicParsing="false" onAction="#logoutAction" style="-fx-background-color: #ffc401;" text="Log Out" textFill="#171717">
               <font>
                  <Font name="Candara" size="22.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
